@{
    Layout = "_Layout";
    ViewData["Title"] = "Add a student";
}
<h1>@ViewData["Title"]</h1>

@if (@Model.Count == 0)
{
  <h3>No students have been added yet!</h3>
}

@foreach (var student in Model)
{
  <h3>@student._name
  <small>@student._number</small></h3>
}

<p><a href='/'>Back home</a></p>
<p><a href='/students/new'>Add a new student</a></p>
